cp winbox.desktop /usr/share/applications/winbox.desktop

mkdir /etc/winbox/ /usr/bin/ -p

cp winbox.ico /etc/winbox/winbox.ico
cp winbox.exe /usr/bin/winbox.exe

chown root. /etc/winbox/winbox.ico
chmod a+r   /etc/winbox/winbox.ico

chown root. /usr/bin/winbox.exe 
chmod a+r   /usr/bin/winbox.exe
sudo wget -q http://www.mikrotik.com/download/winbox.exe -O /usr/bin/winbox.exe

