cp winbox.desktop $HOME/.local/usr/share/applications/winbox.desktop

mkdir /etc/winbox/ /usr/bin/ -p

cp winbox.ico /etc/winbox/winbox.ico
cp winbox.exe /usr/bin/winbox.exe

chown root:root /etc/winbox/winbox.ico
chmod a+r   /etc/winbox/winbox.ico

chown root:root /usr/bin/winbox.exe 
chmod a+r   /usr/bin/winbox.exe

